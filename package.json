{
  "name": "tsp-app-monorepo",
  "version": "1.0.0",
  "description": "TSP App Monorepo",
  "private": true,
  "workspaces": [
    "backend",
    "tsp-app"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=tsp-app"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "typescript",
    "nextjs",
    "fastify",
    "photo-upload",
    "google-drive"
  ],
  "author": "TSP Team",
  "license": "MIT"
}
